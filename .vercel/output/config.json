{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/callback\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/logout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/user\\/createUser\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/user\\/me\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/user\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}